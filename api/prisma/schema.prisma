generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pet {
  id          Int       @id @default(autoincrement())
  name        String
  type        String
  tutorName   String
  breed       String
  dateOfBirth Int
  createdAt   DateTime? @default(now())
  updatedAt   DateTime? @default(now())

  @@map("pets")
}
